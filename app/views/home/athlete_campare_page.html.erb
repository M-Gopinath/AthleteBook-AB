<h3 align="center">Campare Athlete</h3>


<% if @message.present? %>
	<h2 align="center"><%= @message %></h2>
<% else %>
  <div class="col-1-5 hero-feature">
    <div class="thumbnail">
      <img src="/assets/800x500.png" alt="">
      <div class="caption">
        <%#= image_tag athlete.public_image.url %>
        <p class="text-bold">Public Image :</p>
        <p class="text-bold">Sports Name :</p>
        <p class="text-bold">Address :</p>
        <p class="text-bold">College :</p>
        <p class="text-bold">Exp :</p>
        <p class="text-bold">Description :</p>
      </div>
    </div>
  </div>
  <% @athlete.each do |athlete|%>
  <div class="col-1-5 hero-feature">
    <div class="thumbnail">
      <img src="/assets/800x500.png" alt="">
      <div class="caption">
       <%#= image_tag athlete.public_image.url %>
        <p><%= athlete.public_image.present? ? athlete.public_image : "Public Image Empty" %></p>
        <p><%= athlete.sports_name.present? ? athlete.sports_name : "Sports Name Empty" %></p>
        <p><%= athlete.address.present? ? athlete.address : "Address Empty"%></p>
        <p><%= athlete.college.present? ? athlete.college : "College Empty"%></p>
        <p><%= athlete.exp.present? ? athlete.exp : "Exp Empty"%></p>
        <p><%= athlete.description.present? ? athlete.description : "Description Empty"%></p>
      </div>
    </div>
  </div>
  <% end %>
<% end %>
	